class MapNode:

	def __init__ (self, uniqid, neighbors):
		self.uniqid = uniqid
		self.neighbors = neighbors
		self.color = None

	def SetColor(self,color):
		self.color = color

	def __repr__(self):
		return "Node " + str(self.uniqid)+ " with neighbors " + str(self.neighbors) + " and color " + str(self.color) + "."
	def __str__(self):
		return "Node " + str(self.uniqid)+ " with neighbors " + str(self.neighbors) + " and color " + str(self.color) + "."
